    <script>
        function fncClear() {
            $('#Name').val('');
            $('#FatherName').val('');
            $('#txtDate').val('');
            $('#Nationality').val('');
            $('#Qualification').val('');
            $('#City').val('');
            $('#PinCode').val('');
            $('#PhoneNo').val('');
            $('#Email').val('');
            $('#Mobile').val('');
            $('#Profession').val('');
            $('#OtherDetails').val('');
            $('#txtPassword').val('');
        }

        $("#accordion").accordion({
            active: false,
            collapsible: true,
            heightStyle: "content",
            header: "div.accordianheader"
        });

        //Date Picker
        $(function () {
            $('#txtDate').datepicker({
                dateFormat: 'dd-mm-yy',
                changeMonth: true,
                changeYear: true,
                yearRange: "-80:+10",
                //minDate: new Date(),
                inline: true
            });
        });

        //Mobile Validation
        $('#Mobile').on('input', function (event) {
            this.value = this.value.replace(/[^0-9]/g, '');
            var maxLength = 10;
            $(this).prop("maxlength", maxLength);
        });

        //Phone Validation
        $('#PhoneNo').on('input', function (event) {
            this.value = this.value.replace(/[^0-9]/g, '');
            var maxLength = 15;
            $(this).prop("maxlength", maxLength);
        });
        //Pincode Validation
        $('#PinCode').on('input', function (event) {
            this.value = this.value.replace(/[^0-9]/g, '');
            var maxLength = 6;
            $(this).prop("maxlength", maxLength);
        });

        function ValidateEmail() {
            var email = document.getElementById("Email").value;
            var lblError = document.getElementById("Error");
            lblError.innerHTML = "";
            var expr = /^([\w-\.]+)@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
            if (!expr.test(email)) {
                lblError.innerHTML = "Invalid email address.";
            }
            else {
                lblError.innerHTML = "";
            }
        }
    </script>
    <script type="text/javascript">
        $('#txtPassword').keyup(function (e) {
            var color = "";
            var strength = "";
            var strongRegex = /^(?=.*\d)(?=.*[!@@#$%^&*]+)(?![.\n])(?=.*[A-Z])(?=.*[a-z]).{8,}$/;
            // Use the literal regex notation so as not to double escape \ symbols!  Else,
            // var strongRegex = RegExp("^(?=.*\\d)(?=.*[!@@#$%^&*]+)(?![.\n])(?=.*[A-Z])(?=.*[a-z]).{8,}$");
            if ($(this).val().length < 8) {   // no need in the enoughRegex
                //$('#password_strength').html('More Characters');
                strength = "Minimum 8 charecters";

            }
            else if (strongRegex.test($(this).val())) {
                strength = "Weak Password";
            }
            else {
                strength = "Good Password";
            }
            if (strength == "Minimum 8 charecters") {
                color = "Orange";
            }
            if (strength == "Weak Password") {
                color = "Red";
            }
            if (strength == "Good Password") {
                color = "darkgreen";
            }

            $("#password_strength").html(strength);
            $("#password_strength").css("color", color);
            return true;
        });
