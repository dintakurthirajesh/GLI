@model  List<GLI.GlobalEntity.MemberShip>
@{
    ViewData["Title"] = "GetDepartment";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<div class="page-content">
    @*style="min-height:1269px"*@
    <div class="portlet box blue">
        <div class="portlet-title">
            <div class="caption">
                <i class="fa fa-cogs"></i>Membership
            </div>
        </div>
        <div class="portlet-body">
            @*<div class="row">
                    <div class="col-md-6">
                        <div class="btn-group">
                            <button id="sample_editable_1_new" class="btn blue" onclick="@("window.location.href='" + @Url.Action("Arbitration", "Admin") + "'");">
                                Add New <i class="fa fa-plus"></i>
                            </button>
                        </div>
                    </div>
                </div>*@
            <br />
            <div class="table-responsive">
                <table class="table table-striped table-bordered">
                    <thead>
                        <tr>
                            <th>
                                S.No
                            </th>
                            <th>
                                Name
                            </th>
                            <th>
                                FatherName
                            </th>
                            <th>
                                DOB
                            </th>
                            <th>
                                Nationality
                            </th>
                            <th>
                                Country
                            </th>
                            <th>
                                Qualification
                            </th>

                            <th>
                                City
                            </th>
                            <th>
                                Pin
                            </th>
                            <th>
                                Email
                            </th>
                            <th>
                                MobileNo
                            </th>
                            <th>
                                Profession
                            </th>
                            <th>
                                Created Date
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @{ int rno = 1; }

                        @foreach (var item in Model)
                        {
                            <tr role="row" class="odd">
                                <td class="sorting_1">
                                    @rno
                                </td>
                                <td>
                                    @item.Name
                                </td>
                                <td>
                                    @item.FatherName
                                </td>
                                <td>
                                    @item.DOB
                                </td>
                                <td>
                                    @item.Nationality
                                </td>
                                <td>
                                    @item.Country
                                </td>
                                <td>
                                    @item.Qualification
                                </td>

                                <td>
                                    @item.City
                                </td>
                                <td>
                                    @item.Pin
                                </td>
                                <td>
                                    @item.Email
                                </td>
                                <td>
                                    @item.MobileNo
                                </td>
                                <td>
                                    @item.Profession
                                </td>
                                <td>
                                    @item.Date
                                </td>
                            </tr>

                            rno = rno + 1;
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>



